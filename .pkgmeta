package-as: GuildLogistics

manual-changelog: CHANGELOG.md

# Approche inclusive : on spécifie explicitement ce qu'on veut
include:
    - GuildLogistics/

# Ignorer seulement les fichiers/dossiers de développement à la racine
ignore:
    - .github/
    - .git/
    - .gitignore
    - .gitlab/
    - .vscode/
    - README.md
    - cliff.toml
    - LATEST_CHANGELOG.md
    - CHANGELOG.md
    - .pkgmeta

# Déplacer le contenu de GuildLogistics/ à la racine de l'archive
move-folders:
    GuildLogistics: /
