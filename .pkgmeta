package-as: GuildLogistics

manual-changelog: CHANGELOG.md

# Utilisation d'include pour être explicite sur ce qu'on veut
include:
    - GuildLogistics/

# Ignorer les fichiers de développement
ignore:
    - .github/
    - .git/
    - .gitignore
    - .gitlab/
    - .vscode/
    - README.md
    - cliff.toml
    - LATEST_CHANGELOG.md
    - CHANGELOG.md
    - .pkgmeta

# Remonter le contenu de GuildLogistics à la racine du package
move-folders:
    GuildLogistics: /
